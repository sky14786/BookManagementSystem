<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" style="-fx-background-color: #ffffff;"
	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.kh.bms.view.SignUpView">
	<children>
		<Button fx:id="btnCreate" layoutX="155.0" layoutY="315.0"
			mnemonicParsing="false" onAction="#create" prefHeight="38.0"
			prefWidth="89.0" style="-fx-background-color: #E6E6FA;" text="회원가입"
			textFill="GREY">
			<font>
				<Font name="NanumSquare Regular" size="16.0" />
			</font>
		</Button>
		<Button fx:id="btnBack" layoutX="347.0" layoutY="315.0"
			mnemonicParsing="false" prefHeight="38.0" prefWidth="89.0"
			style="-fx-background-color: #E6E6FA;" text="뒤로가기" textFill="GREY">
			<font>
				<Font name="NanumSquare Regular" size="16.0" />
			</font>
		</Button>
		<TextField fx:id="tfId" layoutX="198.0" layoutY="84.0"
			prefHeight="31.0" prefWidth="212.0" />
		<Label alignment="CENTER_RIGHT" layoutX="119.0" layoutY="84.0"
			prefHeight="31.0" prefWidth="70.0" text="ID :" textAlignment="CENTER"
			textFill="#575757">
			<font>
				<Font name="NanumSquare Regular" size="16.0" />
			</font>
		</Label>
		<Label alignment="CENTER_RIGHT" layoutX="119.0" layoutY="128.0"
			prefHeight="31.0" prefWidth="70.0" text="PW :" textAlignment="CENTER"
			textFill="#575757">
			<font>
				<Font name="NanumSquare Regular" size="16.0" />
			</font>
		</Label>
		<TextField fx:id="tfName" layoutX="198.0" layoutY="172.0"
			prefHeight="31.0" prefWidth="212.0" />
		<Label alignment="CENTER_RIGHT" layoutX="119.0" layoutY="172.0"
			prefHeight="31.0" prefWidth="70.0" text="이름 :" textAlignment="CENTER"
			textFill="#575757">
			<font>
				<Font name="NanumSquare Regular" size="16.0" />
			</font>
		</Label>
		<Label alignment="CENTER" layoutX="182.0" layoutY="25.0"
			prefHeight="48.0" prefWidth="237.0" text="Sign Up"
			textAlignment="CENTER" textFill="#575757">
			<font>
				<Font size="24.0" />
			</font>
		</Label>
		<CheckBox fx:id="checkAgree" layoutX="436.0" layoutY="275.0"
			mnemonicParsing="false" text="동의합니다.">
			<font>
				<Font name="NanumSquare Regular" size="13.0" />
			</font>
		</CheckBox>
		<ComboBox fx:id="comGender" layoutX="232.0" layoutY="260.0"
			onAction="#comboChanged" prefHeight="31.0" prefWidth="137.0"
			promptText="성별" />
		<TextField fx:id="tfAddr" layoutX="198.0" layoutY="215.0"
			prefHeight="31.0" prefWidth="212.0" />
		<Label alignment="CENTER_RIGHT" layoutX="119.0" layoutY="215.0"
			prefHeight="31.0" prefWidth="70.0" text="주소 :" textAlignment="CENTER"
			textFill="#575757">
			<font>
				<Font name="NanumSquare Regular" size="16.0" />
			</font>
		</Label>
		<PasswordField fx:id="tfPw" layoutX="198.0"
			layoutY="128.0" prefHeight="31.0" prefWidth="212.0" />
		<Label layoutX="500.0" layoutY="84.0" prefHeight="31.0"
			prefWidth="70.0" textAlignment="CENTER" textFill="#575757"
			fx:id="lDuText">
			<font>
				<Font name="NanumSquare Regular" size="16.0" />
			</font>
		</Label>
		<Button fx:id="btnDuplicateCheck" layoutX="419.0"
			layoutY="83.0" mnemonicParsing="false" onAction="#duplicateCheck"
			prefHeight="32.0" prefWidth="70.0"
			style="-fx-background-color: #E6E6FA;" text="중복확인" textFill="GREY">
			<font>
				<Font name="NanumSquare Regular" size="13.0" />
			</font>
		</Button>
	</children>
</Pane>
